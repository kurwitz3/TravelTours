<meta name="viewport" content="width=device-width, initial-scale=1">
<style>body  {
    background-image: url("/boats.webp");
    background-size: cover;
    <link rel="stylesheet" href="/stylesheets/signin.css" />
  } </style>
  <div class="topnav">
  <a class="active" href="#home">Home</a>
  <a href="#about">Log Out</a>
</div>
<div align="center">
<h1>Create Account</h1>
<%= form_for @client do |f| %>
 <table>
  <tr>
   <td><h2>Name:</h2></td> <td><%= f.text_field :name %></td><br>
  </tr>
  <tr>
   <td><h2>Spending Money(Dollar amount):</h2></td> <td><%= f.number_field :spending_money %></td><br>
  </tr>
  <tr>
   <td><h2>Email :</h2></td> <td><%= f.text_field :email %></td><br>
  </tr>
  <tr>
   <td><h2>Password :</h2></td> <td><%= f.text_field :password %></td><br>
  </tr>
  <tr>
   <td><h2>Password Conformation:</h2></td> <td><%= f.text_field :password_conformation %></td><br>
  </tr>
  <tr>
   <td><%= f.submit %></td>
  </tr>
  <% end %>
 </table>
 </form>
 </div>
 



















